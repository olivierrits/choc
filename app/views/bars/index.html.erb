

<div class="bar-index">
  <div class="bar-search">
    <%= render 'shared/search_bar' %>
  </div>

  <div class="bar-container">
  <div class="bar-cards">
    <% @bars.each do |bar| %>
      <%= render 'bar', bar: bar, user: @user  %>

      <% end %>
  </div>
  <% flash.each do |type, msg| %>
  <div class="alert alert-info">
    <%= msg %>
  </div>
<% end %>
  </div>
</div>
